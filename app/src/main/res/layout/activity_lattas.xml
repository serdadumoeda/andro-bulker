<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ddd"
    android:orientation="vertical"
    android:padding="16dp">

    <android.support.v7.widget.CardView
        android:onClick="lembaga"
        android:foreground="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:id="@+id/android_card_view_example"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="0dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff"
            android:elevation="4dp"
            android:padding="16dp">


            <TextView
                android:id="@+id/cardview_list_title"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:paddingBottom="5dp"
                android:text="Kelembagaan"
                android:textStyle="bold"
                android:textSize="24sp" />



        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:onClick="intala"
        android:foreground="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:id="@+id/android_card_view_example1"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="0dp"
        android:layout_marginTop="16dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff"
            android:elevation="4dp"
            android:padding="16dp">


            <TextView
                android:id="@+id/cardview_list_title1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:paddingBottom="5dp"
                android:text="Instruktur dan Tenaga Pelatihan"
                android:textStyle="bold"
                android:textSize="24sp" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:onClick="stankom"
        android:foreground="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:id="@+id/android_card_view_example2"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="0dp"
        android:layout_marginTop="16dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff"
            android:elevation="4dp"
            android:padding="16dp">


            <TextView
                android:id="@+id/cardview_list_title2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:paddingBottom="6dp"
                android:text="Standarisasi Kompetensi"
                android:textStyle="bold"
                android:textSize="24sp" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:onClick="magang"
        android:foreground="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:id="@+id/android_card_view_example8"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="0dp"
        android:layout_marginTop="16dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff"
            android:elevation="4dp"
            android:padding="16dp">


            <TextView
                android:id="@+id/cardview_list_title5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:paddingBottom="6dp"
                android:text="Pemagangan"
                android:textStyle="bold"
                android:textSize="24sp" />


        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:onClick="prod"
        android:foreground="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:id="@+id/android_card_view_example3"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="0dp"
        android:layout_marginTop="16dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff"
            android:elevation="4dp"
            android:padding="16dp">


            <TextView
                android:id="@+id/cardview_list_title3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:paddingBottom="5dp"
                android:text="Produktivitas"
                android:textStyle="bold"
                android:textSize="24sp" />


        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:onClick="bnsp"
        android:foreground="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:id="@+id/android_card_view_example7"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="0dp"
        android:layout_marginTop="16dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff"
            android:elevation="4dp"
            android:padding="16dp">


            <TextView
                android:id="@+id/cardview_list_title7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:paddingBottom="5dp"
                android:text="BNSP"
                android:textStyle="bold"
                android:textSize="24sp" />


        </RelativeLayout>

    </android.support.v7.widget.CardView>



</LinearLayout>